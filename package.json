{
  "name": "amp-story-ads-preview",
  "version": "1.0.0",
  "author": "The AMPHTML Authors",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -Rf dist",
    "build": "yarn run --silent x bin/build.js",
    "lint": "`yarn bin`/eslint bin lib test src",
    "postinstall": "./bin/postinstall.sh",
    "serve": "yarn run --silent x bin/serve.js",
    "test": "`yarn bin`/mocha --require ignore-styles --require esm",
    "watch": "yarn run --silent x bin/watch.js",
    "x": "node -r ignore-styles -r esm"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "yarn x"
    },
    "script": "bin/serve.js",
    "watch": [
      "bin",
      "lib",
      "src"
    ],
    "events": {
      "restart": "yarn build"
    }
  },
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "codemirror": "5.47.0",
    "dompurify": "1.0.10",
    "lit-html": "1.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-async-to-generator": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@popeindustries/lit-html-server": "1.2.0",
    "chai": "4.2.0",
    "colors": "1.3.3",
    "cssnano": "4.1.10",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-disable": "1.0.4",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-notice": "0.7.8",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.4.0",
    "esm": "3.2.25",
    "fancy-log": "1.3.3",
    "fast-glob": "2.2.7",
    "finalhandler": "^1.1.2",
    "fs-extra": "8.0.1",
    "html-minifier": "4.0.0",
    "ignore-styles": "5.0.1",
    "minimist": "1.2.0",
    "mocha": "6.1.4",
    "nodemon": "1.19.1",
    "postcss-import": "12.0.1",
    "postcss-simple-vars": "5.0.2",
    "prettier": "1.17.1",
    "rollup": "1.12.4",
    "rollup-plugin-alias": "1.5.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-ignore": "1.0.5",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-postcss": "2.0.3",
    "serve-static": "1.14.1",
    "terser": "4.0.0"
  }
}
